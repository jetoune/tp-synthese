Liste des projets :

{% if  projects %}
	<ul>
		{% for project in projects %}
			<li>
				<p>{{project.name}}</p>
				<p>{{project.startDate|date('d/m/Y')}}</p>
				<p>{{project.endDate|date('d/m/Y')}}</p>
				<p>{{project.rate}}</p>
				<p>{{project.description}}</p>
			</li>
			
		{% endfor %}
	</ul>
{% endif %}